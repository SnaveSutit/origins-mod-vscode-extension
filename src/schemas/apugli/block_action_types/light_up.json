{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://apugli.readthedocs.io/en/latest/types/block_action_types/light_up/",
	"type": "object",
	"properties": {
		"burn_time": {
			"type": "integer",
			"default": 1600
		},
		"brew_time": {
			"type": "integer",
			"default": 20
		},
		"light_campfire": {
			"type": "boolean",
			"default": true
		},
		"particle": {
			"$ref": "$ref(apoli:types/autocomplete_helpers/particle_identifier)"
		},
		"particle_count": {
			"type": "integer"
		},
		"sound": {
			"$ref": "$ref(apoli:types/autocomplete_helpers/sound_identifier)"
		}
	}
}