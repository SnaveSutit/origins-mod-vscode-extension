{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://apugli.readthedocs.io/en/latest/types/entity_action_types/spawn_particles/",
	"type": "object",
	"required": ["particle", "count"],
	"properties": {
		"type": {
			"description": "### ⚠️ Requires [Apugli](https://apugli.readthedocs.io/en/latest/) to be installed! ⚠️\n\n",
			"markdownDescription": "### ⚠️ Requires [Apugli](https://apugli.readthedocs.io/en/latest/) to be installed! ⚠️\n\n"
		},
		"particle": {
			"$ref": "$ref(apoli:types/particle_effect)"
		},
		"count": {
			"type": "integer"
		},
		"speed": {
			"type": "number",
			"default": 0
		},
		"force": {
			"type": "boolean",
			"default": false
		},
		"velocity": {
			"$ref": "$ref(apoli:types/vector)"
		},
		"spread": {
			"default": "{\"x\": 0.5, \"y\": 0.25, \"z\": 0.5}",
			"$ref": "$ref(apoli:types/vector)"
		},
		"offset_y": {
			"type": "number",
			"default": 0.5
		}
	}
}
