{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://apugli.readthedocs.io/en/latest/types/entity_action_types/fire_projectile/",
	"type": "object",
	"properties": {
		"type": {
			"description": "### ⚠️ Requires [Apugli](https://apugli.readthedocs.io/en/latest/) to be installed! ⚠️\n\n",
			"markdownDescription": "### ⚠️ Requires [Apugli](https://apugli.readthedocs.io/en/latest/) to be installed! ⚠️\n\n"
		},
		"count": {
			"type": "integer",
			"default": 1
		},
		"speed": {
			"type": "number",
			"default": 1.5
		},
		"divergence": {
			"type": "number",
			"default": 1
		},
		"sound": {
			"$ref": "$ref(apoli:types/autocomplete_helpers/sound_identifier)"
		},
		"tag": {
			"$ref": "$ref(apoli:types/nbt)"
		},
		"bientity_action": {
			"$ref": "$ref(apoli:bientity_action)"
		}
	}
}
