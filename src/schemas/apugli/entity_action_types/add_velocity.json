{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://apugli.readthedocs.io/en/latest/types/entity_action_types/add_velocity/",
	"type": "object",
	"properties": {
		"type": {
			"description": "### ⚠️ Requires [Apugli](https://apugli.readthedocs.io/en/latest/) to be installed! ⚠️\n\n",
			"markdownDescription": "### ⚠️ Requires [Apugli](https://apugli.readthedocs.io/en/latest/) to be installed! ⚠️\n\n"
		},
		"x": {
			"type": "number",
			"default": 0
		},
		"y": {
			"type": "number",
			"default": 0
		},
		"z": {
			"type": "number",
			"default": 0
		},
		"horizontal_modifier": {
			"$ref": "$ref(apoli:types/attribute_modifier)"
		},
		"horizontal_modifiers": {
			"type": "array",
			"items": {
				"$ref": "$ref(apoli:types/attribute_modifier)"
			}
		},
		"vertical_modifier": {
			"$ref": "$ref(apoli:types/attribute_modifier)"
		},
		"vertical_modifiers": {
			"type": "array",
			"items": {
				"$ref": "$ref(apoli:types/attribute_modifier)"
			}
		},
		"horizontal_post_modifier": {
			"$ref": "$ref(apoli:types/attribute_modifier)"
		},
		"horizontal_post_modifiers": {
			"type": "array",
			"items": {
				"$ref": "$ref(apoli:types/attribute_modifier)"
			}
		},
		"vertical_post_modifier": {
			"$ref": "$ref(apoli:types/attribute_modifier)"
		},
		"vertical_post_modifiers": {
			"type": "array",
			"items": {
				"$ref": "$ref(apoli:types/attribute_modifier)"
			}
		},
		"space": {
			"type": "space",
			"default": "world"
		},
		"client": {
			"type": "boolean",
			"default": true
		},
		"server": {
			"type": "boolean",
			"default": true
		},
		"set": {
			"type": "boolean",
			"default": false
		}
	}
}
