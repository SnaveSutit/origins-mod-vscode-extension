{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://apugli.readthedocs.io/en/latest/types/entity_condition_types/velocity/",
	"type": "object",
	"properties": {
		"x": {
			"type": "number"
		},
		"y": {
			"type": "number"
		},
		"z": {
			"type": "number"
		},
		"axes": {
			"type": "array"
		},
		"comparison": {
			"default": ">=",
			"$ref": "$ref(apoli:types/comparison)"
		},
		"compare_to": {
			"type": "number"
		}
	}
}