{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://apugli.readthedocs.io/en/latest/types/entity_condition_types/base_enchantment/",
	"type": "object",
	"required": [
		"enchantment",
		"compare_to"
	],
	"properties": {
		"enchantment": {
			"$ref": "$ref(apoli:types/identifier)"
		},
		"calculation": {
			"default": "sum",
			"$ref": "$ref(apoli:types/identifier)"
		},
		"comparison": {
			"default": ">=",
			"$ref": "$ref(apoli:types/comparison)"
		},
		"compare_to": {
			"type": "integer"
		}
	}
}