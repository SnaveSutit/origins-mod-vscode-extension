{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://apugli.readthedocs.io/en/latest/types/data_types/weighted_sound_event/",
	"type": "object",
	"oneOf": [
		{
			"required": ["sound"]
		},
		{
			"required": ["sounds"]
		}
	],
	"properties": {
		"sound": {
			"$ref": "$ref(apugli:types/sound_event_with_volume_and_pitch)"
		},
		"sounds": {
			"type": "array",
			"items": {
				"$ref": "$ref(apugli:types/sound_event_with_volume_and_pitch)"
			}
		},
		"weight": {
			"type": "number",
			"default": 1
		}
	}
}
