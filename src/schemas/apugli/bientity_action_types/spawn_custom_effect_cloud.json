{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://apugli.readthedocs.io/en/latest/types/bientity_action_types/spawn_custom_effect_cloud/",
	"type": "object",
	"required": ["particle"],
	"properties": {
		"type": {
			"description": "### ⚠️ Requires [Apugli](https://apugli.readthedocs.io/en/latest/) to be installed! ⚠️\n\n",
			"markdownDescription": "### ⚠️ Requires [Apugli](https://apugli.readthedocs.io/en/latest/) to be installed! ⚠️\n\n"
		},
		"entity_id": {
			"$ref": "$ref(apoli:types/autocomplete_helpers/entity_identifier)"
		},
		"radius": {
			"type": "number",
			"default": 3
		},
		"radius_on_use": {
			"type": "number",
			"default": 3
		},
		"radius_per_tick": {
			"type": "number",
			"default": 3
		},
		"wait_time": {
			"type": "integer",
			"default": 10
		},
		"duration": {
			"type": "integer",
			"default": 600
		},
		"reapplication_delay": {
			"type": "integer",
			"default": 20
		},
		"particle": {
			"$ref": "$ref(apoli:types/particle_effect)"
		},
		"height_increase": {
			"type": "number",
			"default": 0
		},
		"powers_to_apply": {
			"type": "array",
			"default": "0.0"
		},
		"owner_cloud_bientity_action": {
			"$ref": "$ref(apoli:bientity_action)"
		},
		"cloud_target_bientity_action": {
			"$ref": "$ref(apoli:bientity_action)"
		},
		"owner_target_bientity_action": {
			"$ref": "$ref(apoli:bientity_action)"
		},
		"bientity_condition": {
			"$ref": "$ref(apoli:bientity_condition)"
		},
		"owner_target_bientity_condition": {
			"$ref": "$ref(apoli:bientity_condition)"
		},
		"spawn_target": {
			"type": "string",
			"default": "target",
			"enum": ["actor", "ACTOR", "target", "TARGET"]
		}
	}
}
