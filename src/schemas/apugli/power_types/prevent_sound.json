{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://apugli.readthedocs.io/en/latest/types/power_types/prevent_sound/",
	"type": "object",
	"properties": {
		"category": {
			"type": "string",
			"enum": [
				"ambient",
				"blocks",
				"hostile",
				"master",
				"music",
				"neutral",
				"players",
				"records",
				"voice",
				"weather"
			]
		},
		"categories": {
			"type": "array"
		},
		"sound": {
			"$ref": "$ref(apoli:types/autocomplete_helpers/sound_identifier)"
		},
		"sounds": {
			"type": "array",
			"items": {
				"$ref": "$ref(apoli:types/autocomplete_helpers/sound_identifier)"
			}
		},
		"whitelist": {
			"type": "array"
		}
	}
}