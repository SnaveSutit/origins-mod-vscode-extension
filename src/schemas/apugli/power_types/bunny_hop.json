{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://apugli.readthedocs.io/en/latest/types/power_types/bunny_hop/",
	"type": "object",
	"required": ["min", "max", "hud_render"],
	"properties": {
		"type": {
			"description": "### ⚠️ Requires [Apugli](https://apugli.readthedocs.io/en/latest/) to be installed! ⚠️\n\n",
			"markdownDescription": "### ⚠️ Requires [Apugli](https://apugli.readthedocs.io/en/latest/) to be installed! ⚠️\n\n"
		},
		"min": {
			"type": "integer"
		},
		"max": {
			"type": "integer"
		},
		"hud_render": {
			"$ref": "$ref(apoli:types/hud_render)"
		},
		"start_value": {
			"type": "integer"
		},
		"min_action": {
			"$ref": "$ref(apoli:entity_action)"
		},
		"max_action": {
			"$ref": "$ref(apoli:entity_action)"
		},
		"increase_per_tick": {
			"type": "number",
			"default": 0.000375
		},
		"tick_rate": {
			"type": "integer",
			"default": 10
		}
	}
}
