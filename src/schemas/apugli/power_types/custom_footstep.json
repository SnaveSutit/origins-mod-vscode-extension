{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://apugli.readthedocs.io/en/latest/types/power_types/custom_footstep/",
	"type": "object",
	"properties": {
		"muted": {
			"type": "boolean",
			"default": false
		},
		"sound": {
			"$ref": "$ref(apugli:types/weighted_sound_event)"
		},
		"sounds": {
			"type": "array",
			"items": {
				"$ref": "$ref(apugli:types/weighted_sound_event)"
			}
		},
		"volume": {
			"type": "number",
			"default": 1
		},
		"pitch": {
			"type": "number",
			"default": 1
		}
	}
}