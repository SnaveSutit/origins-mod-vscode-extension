{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://apugli.readthedocs.io/en/latest/types/power_types/modify_durability_change/",
	"type": "object",
	"properties": {
		"item_condition": {
			"$ref": "$ref(apoli:item_condition)"
		},
		"comparison": {
			"default": ">=",
			"$ref": "$ref(apoli:types/comparison)"
		},
		"compare_to": {
			"type": "number",
			"default": -2147483648
		},
		"comparisons": {
			"type": "array",
			"items": {
				"$ref": "$ref(apugli:types/comparison_map)"
			}
		},
		"modifier": {
			"$ref": "$ref(apoli:types/attribute_modifier)"
		},
		"modifiers": {
			"type": "array",
			"items": {
				"$ref": "$ref(apoli:types/attribute_modifier)"
			}
		}
	}
}
