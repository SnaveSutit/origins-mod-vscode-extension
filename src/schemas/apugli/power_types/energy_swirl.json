{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://apugli.readthedocs.io/en/latest/types/power_types/energy_swirl/",
	"type": "object",
	"properties": {
		"texture_location": {
			"$ref": "$ref(apoli:types/autocomplete_helpers/texture_location)"
		},
		"texture_url": {
			"type": "string",
			"enum": [
				"texture_location"
			]
		},
		"size": {
			"type": "number",
			"default": 1
		},
		"speed": {
			"type": "number",
			"default": 0.01
		}
	}
}