{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://apugli.readthedocs.io/en/latest/types/power_types/modify_equipped_item_render/",
	"type": "object",
	"required": ["slot", "stack"],
	"properties": {
		"type": {
			"description": "### ⚠️ Requires [Apugli](https://apugli.readthedocs.io/en/latest/) to be installed! ⚠️\n\n",
			"markdownDescription": "### ⚠️ Requires [Apugli](https://apugli.readthedocs.io/en/latest/) to be installed! ⚠️\n\n"
		},
		"slot": {
			"$ref": "$ref(apoli:types/slot)"
		},
		"stack": {
			"$ref": "$ref(apoli:types/item_stack)"
		},
		"scale": {
			"type": "number",
			"default": 1
		},
		"override": {
			"type": "boolean",
			"default": false
		},
		"merge_with_held": {
			"type": "boolean",
			"default": false
		}
	}
}
