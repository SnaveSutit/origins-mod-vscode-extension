{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://origins.readthedocs.io/en/latest/types/entity_condition_types/raycast/",
	"type": "object",
	"properties": {
		"distance": {
			"type": "number"
		},
		"block": {
			"type": "boolean",
			"default": true
		},
		"entity": {
			"type": "boolean",
			"default": true
		},
		"shape_type": {
			"$ref": "$ref(apoli:types/shape_type)"
		},
		"fluid_handling": {
			"$ref": "$ref(apoli:types/fluid_handling)"
		},
		"space": {
			"$ref": "$ref(apoli:types/space)"
		},
		"direction": {
			"type": "object"
		},
		"match_bientity_condition": {
			"$ref": "$ref(apoli:bientity_condition)"
		},
		"hit_bientity_condition": {
			"$ref": "$ref(apoli:bientity_condition)"
		},
		"entity_distance": {
			"type": "number"
		},
		"block_condition": {
			"$ref": "$ref(apoli:block_condition)"
		},
		"block_distance": {
			"type": "number"
		}
	}
}
