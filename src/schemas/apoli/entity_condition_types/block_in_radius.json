{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://origins.readthedocs.io/en/latest/types/entity_condition_types/block_in_radius/",
	"type": "object",
	"required": ["block_condition", "radius"],
	"properties": {
		"block_condition": {
			"$ref": "$ref(apoli:block_condition)"
		},
		"radius": {
			"type": "integer"
		},
		"shape": {
			"type": "string",
			"default": "\"cube\""
		},
		"comparison": {
			"$ref": "$ref(apoli:types/comparison)"
		},
		"compare_to": {
			"type": "integer",
			"default": 1
		}
	}
}
