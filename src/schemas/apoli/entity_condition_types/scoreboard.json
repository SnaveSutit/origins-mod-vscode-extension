	"$docsUrl": "https://origins.readthedocs.io/en/latest/types/entity_condition_types/scoreboard/",
	"type": "object",
	"required": ["objective", "comparison", "compare_to"],
	"properties": {
		"name": {
			"type": "string"
		},
		"objective": {
			"type": "string"
		},
		"comparison": {
			"$ref": "$ref(apoli:types/comparison)"
		},
		"compare_to": {
			"type": "integer"
		}
	}
}
