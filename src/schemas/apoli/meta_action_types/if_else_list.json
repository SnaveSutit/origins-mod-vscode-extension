{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://origins.readthedocs.io/en/latest/types/meta_action_types/if_else_list/",
	"type": "object",
	"required": ["actions"],
	"properties": {
		"actions": {
			"type": "array",
			"items": {
				"type": "object",
				"required": ["action", "condition"],
				"properties": {
					"action": {
						"$ref": "$actionRef"
					},
					"condition": {
						"$ref": "$conditionRef"
					}
				}
			}
		}
	}
}
