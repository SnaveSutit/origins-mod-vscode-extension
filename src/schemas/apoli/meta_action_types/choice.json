{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://origins.readthedocs.io/en/latest/types/meta_action_types/choice/",
	"type": "object",
	"required": ["actions"],
	"properties": {
		"actions": {
			"type": "array",
			"items": {
				"type": "object",
				"required": ["element", "weight"],
				"properties": {
					"element": {
						"$ref": "$actionRef"
					},
					"weight": {
						"type": "number"
					}
				}
			}
		}
	}
}
