{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://origins.readthedocs.io/en/latest/types/data_types/status_effect_instance/",
	"type": "object",
	"required": ["effect"],
	"properties": {
		"effect": {
			"$ref": "$ref(apoli:types/autocomplete_helpers/status_effect_identifier)"
		},
		"duration": {
			"type": "integer",
			"default": 100
		},
		"amplifier": {
			"type": "integer",
			"default": 0
		},
		"is_ambient": {
			"type": "boolean",
			"default": false
		},
		"show_particles": {
			"type": "boolean",
			"default": true
		},
		"show_icon": {
			"type": "boolean",
			"default": true
		}
	}
}
