{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://origins.readthedocs.io/en/latest/types/data_types/damage_source/",
	"type": "object",
	"required": ["name"],
	"properties": {
		"name": {
			"type": "string"
		},
		"bypasses_armor": {
			"type": "boolean",
			"default": false
		},
		"fire": {
			"type": "boolean",
			"default": false
		},
		"unblockable": {
			"type": "boolean",
			"default": false
		},
		"magic": {
			"type": "boolean",
			"default": false
		},
		"out_of_world": {
			"type": "boolean",
			"default": false
		}
	}
}
