{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://origins.readthedocs.io/en/latest/types/power_types/active_self/",
	"type": "object",
	"required": ["entity_action"],
	"properties": {
		"entity_action": {
			"$ref": "$ref(apoli:entity_action)"
		},
		"cooldown": {
			"type": "integer",
			"default": 1
		},
		"hud_render": {
			"type": "hud render",
			"default": "{\"should_render\": false}"
		},
		"key": {
			"type": "key",
			"default": "{\"key\": \"key.origins.primary_active\"}"
		}
	}
}
