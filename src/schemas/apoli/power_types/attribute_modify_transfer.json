{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://origins.readthedocs.io/en/latest/types/power_types/attribute_modify_transfer/",
	"type": "object",
	"required": ["class", "attribute"],
	"properties": {
		"class": {
			"enum": [
				"modify_air_speed",
				"modify_break_speed",
				"modify_crafting",
				"modify_damage_dealt",
				"modify_damage_taken",
				"modify_exhaustion",
				"modify_xp_gain",
				"modify_jump",
				"modify_lava_speed",
				"modify_projectile_damage",
				"modify_slipperiness",
				"modify_status_effect_amplifier",
				"modify_status_effect_duration",
				"modify_swim_speed"
			]
		},
		"attribute": {
			"$ref": "$ref(apoli:types/autocomplete_helpers/attribute_identifier)"
		},
		"multiplier": {
			"type": "number",
			"default": 1.0
		}
	}
}
