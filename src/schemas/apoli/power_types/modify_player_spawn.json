{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://origins.readthedocs.io/en/latest/types/power_types/modify_player_spawn/",
	"type": "object",
	"required": ["dimension"],
	"properties": {
		"dimension": {
			"$ref": "$ref(apoli:types/identifier)"
		},
		"biome": {
			"$ref": "$ref(apoli:types/identifier)"
		},
		"structure": {
			"$ref": "$ref(apoli:types/identifier)"
		},
		"spawn_strategy": {
			"enum": ["default", "center"],
			"default": "default"
		},
		"dimension_distance_multiplier": {
			"type": "number"
		}
	}
}
