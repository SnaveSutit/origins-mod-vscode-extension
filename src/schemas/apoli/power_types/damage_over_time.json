{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://origins.readthedocs.io/en/latest/types/power_types/damage_over_time/",
	"type": "object",
	"required": ["damage"],
	"$IGNORED_PROPERTIES": ["damage_source"],
	"properties": {
		"interval": {
			"type": "integer",
			"default": 20
		},
		"onset_delay": {
			"type": "integer"
		},
		"damage": {
			"type": "number"
		},
		"damage_easy": {
			"type": "number"
		},
		"damage_type": {
			"$ref": "$ref(apoli:types/autocomplete_helpers/damage_type_identifier)",
			"default": "apoli:damage_over_time"
		},
		"protection_enchantment": {
			"$ref": "$ref(apoli:types/autocomplete_helpers/enchantment_identifier)"
		},
		"protection_effectiveness": {
			"type": "number",
			"default": 1.0
		}
	}
}
