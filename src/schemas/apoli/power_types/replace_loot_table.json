{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://origins.readthedocs.io/en/latest/types/power_types/replace_loot_table/",
	"type": "object",
	"required": ["replace"],
	"properties": {
		"replace": {
			"type": "object",
			"patternProperties": {
				"^([a-z0-9_\\/\\-\\.]+:)?[a-z0-9_\\/\\-\\.]+$": {
					"$ref": "$ref(apoli:types/autocomplete_helpers/loot_table_identifier)"
				}
			}
		},
		"bientity_condition": {
			"$ref": "$ref(apoli:bientity_condition)"
		},
		"block_condition": {
			"$ref": "$ref(apoli:block_condition)"
		},
		"item_condition": {
			"$ref": "$ref(apoli:item_condition)"
		},
		"priority": {
			"type": "integer",
			"default": 0
		}
	}
}
