{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://origins.readthedocs.io/en/latest/types/power_types/modify_food/",
	"type": "object",
	"properties": {
		"item_condition": {
			"$ref": "$ref(apoli:item_condition)"
		},
		"item_action": {
			"$ref": "$ref(apoli:item_action)"
		},
		"replace_stack": {
			"$ref": "$ref(apoli:types/item_stack)"
		},
		"food_modifier": {
			"$ref": "$ref(apoli:types/attribute_modifier_arrayable)"
		},
		"food_modifiers": {
			"type": "array",
			"items": {
				"$ref": "$ref(apoli:types/attribute_modifier_arrayable)"
			}
		},
		"saturation_modifier": {
			"$ref": "$ref(apoli:types/attribute_modifier_arrayable)"
		},
		"saturation_modifiers": {
			"type": "array",
			"items": {
				"$ref": "$ref(apoli:types/attribute_modifier_arrayable)"
			}
		},
		"entity_action": {
			"$ref": "$ref(apoli:entity_action)"
		},
		"always_edible": {
			"type": "boolean",
			"default": false
		},
		"prevent_effects": {
			"type": "boolean",
			"default": false
		}
	}
}
