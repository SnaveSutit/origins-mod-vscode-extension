{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://origins.readthedocs.io/en/latest/types/power_types/fire_projectile/",
	"type": "object",
	"required": ["entity_type"],
	"properties": {
		"entity_type": {
			"$ref": "$ref(apoli:types/autocomplete_helpers/entity_identifier)"
		},
		"cooldown": {
			"type": "integer",
			"default": 1
		},
		"hud_render": {
			"$ref": "$ref(apoli:types/hud_render)"
		},
		"count": {
			"type": "integer",
			"default": 1
		},
		"interval": {
			"type": "integer",
			"default": 0
		},
		"start_delay": {
			"type": "integer",
			"default": 0
		},
		"speed": {
			"type": "number",
			"default": 1.5
		},
		"divergence": {
			"type": "number",
			"default": 1.0
		},
		"sound": {
			"$ref": "$ref(apoli:types/autocomplete_helpers/sound_identifier)"
		},
		"tag": {
			"$ref": "$ref(apoli:types/nbt)"
		},
		"key": {
			"$ref": "$ref(apoli:types/key)"
		},
		"projectile_action": {
			"$ref": "$ref(apoli:entity_action)"
		},
		"shooter_action": {
			"$ref": "$ref(apoli:entity_action)"
		}
	}
}
