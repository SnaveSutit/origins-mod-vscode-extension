{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://origins.readthedocs.io/en/latest/types/power_types/action_on_being_used/",
	"type": "object",
	"properties": {
		"bientity_action": {
			"$ref": "$ref(apoli:bientity_action)"
		},
		"held_item_action": {
			"$ref": "$ref(apoli:item_action)"
		},
		"result_item_action": {
			"$ref": "$ref(apoli:item_action)"
		},
		"bientity_condition": {
			"$ref": "$ref(apoli:bientity_condition)"
		},
		"item_condition": {
			"$ref": "$ref(apoli:item_condition)"
		},
		"hands": {
			"$ref": "$ref(apoli:types/hands)"
		},
		"result_stack": {
			"$ref": "$ref(apoli:types/item_stack)"
		},
		"action_result": {
			"$ref": "$ref(apoli:types/action_result)"
		},
		"priority": {
			"type": "integer",
			"default": 0
		}
	}
}
