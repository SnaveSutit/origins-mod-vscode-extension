{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://origins.readthedocs.io/en/latest/json/origin/",
	"type": "object",
	"properties": {
		"powers": {
			"type": "array",
			"items": {
				"$ref": "$(apoli:types/identifier)"
			}
		},
		"icon": {
			"oneOf": [{ "$ref": "$(apoli:types/identifier)" }, { "$ref": "$(apoli:types/item_stack)" }]
		},
		"unchoosable": {
			"type": "boolean",
			"default": false
		},
		"order": {
			"type": "integer"
		},
		"impact": {
			"type": "integer",
			"minimum": 0,
			"maximum": 3
		},
		"name": {
			"type": "string"
		},
		"description": {
			"type": "string"
		},
		"upgrades": {
			"type": "array",
			"items": {
				"$ref": "$(apoli:upgrade)"
			}
		},
		"loading_priority": {
			"type": "integer",
			"default": 0
		}
	}
}
