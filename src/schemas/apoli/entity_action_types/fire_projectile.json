{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://origins.readthedocs.io/en/latest/types/entity_action_types/fire_projectile/",
	"type": "object",
	"required": ["entity_type"],
	"properties": {
		"entity_type": {
			"$ref": "$ref(apoli:types/autocomplete_helpers/entity_identifier)"
		},
		"divergence": {
			"type": "number",
			"default": 1.0
		},
		"speed": {
			"type": "number",
			"default": 1.0
		},
		"count": {
			"type": "integer",
			"default": 1
		},
		"tag": {
			"$ref": "$ref(apoli:types/nbt)"
		},
		"projectile_action": {
			"$ref": "$ref(apoli:entity_action)"
		}
	}
}
