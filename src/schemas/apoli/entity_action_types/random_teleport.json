{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://origins.readthedocs.io/en/latest/types/entity_action_types/random_teleport/",
	"type": "object",
	"properties": {
		"area_width": {
			"type": "number",
			"default": 8.0
		},
		"area_height": {
			"type": "number",
			"default": 8.0
		},
		"heightmap": {
			"type": "string"
		},
		"attempts": {
			"type": "integer"
		},
		"landing_block_condition": {
			"$ref": "$ref(apoli:block_condition)"
		},
		"landing_condition": {
			"$ref": "$ref(apoli:entity_condition)"
		},
		"landing_offset": {
			"$ref": "$ref(apoli:types/vector)"
		},
		"loaded_chunks_only": {
			"type": "boolean",
			"default": true
		},
		"success_action": {
			"$ref": "$ref(apoli:entity_action)"
		},
		"fail_action": {
			"$ref": "$ref(apoli:entity_action)"
		}
	}
}
