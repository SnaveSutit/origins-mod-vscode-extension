{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://origins.readthedocs.io/en/latest/types/entity_action_types/action_on_set/",
	"type": "object",
	"required": ["set", "bientity_action"],
	"properties": {
		"set": {
			"$ref": "$ref(apoli:types/identifier)"
		},
		"bientity_action": {
			"$ref": "$ref(apoli:bientity_action)"
		},
		"bientity_condition": {
			"$ref": "$ref(apoli:bientity_condition)"
		},
		"limit": {
			"type": "integer",
			"default": 0
		},
		"reverse": {
			"type": "boolean",
			"default": false
		}
	}
}
