{
	"$schema": "https://json-schema.org/draft-07/schema#",
	"$docsUrl": "https://origins.readthedocs.io/en/latest/types/entity_action_types/play_sound/",
	"type": "object",
	"required": ["sound"],
	"properties": {
		"sound": {
			"$ref": "$ref(apoli:types/autocomplete_helpers/sound_identifier)"
		},
		"category": {
			"$ref": "$ref(apoli:types/playsound_category)"
		},
		"volume": {
			"type": "number",
			"default": 1.0
		},
		"pitch": {
			"type": "number",
			"default": 1.0
		}
	}
}
